# Boundless Market Configuration
BOUNDLESS_SIGNER_KEY=0xYOUR_PRIVATE_KEY_HERE
BOUNDLESS_OFFCHAIN=false

# S3 Storage Configuration
S3_URL=https://your-bucket.s3.region.amazonaws.com/path
S3_PROVIDER=s3
S3_BUCKET=your-bucket-name
AWS_REGION=us-east-2
S3_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
S3_SECRET_KEY=YOUR_AWS_SECRET_KEY

# Optional: SQLite Database Path (defaults to /data/boundless/boundless_requests.db in Docker)
# SQLITE_DB_PATH=/custom/path/to/db.sqlite

# Optional: Rust logging
RUST_LOG=info
